{
  "name": "app",
  "version": "0.1.0",
  "main": "./src/main.ts",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/src/main.js",
    "build": "tsc",
    "sql": "ts-sql-plugin",
    "watch": "tsc --noEmit --watch",
    "kanel": "npx kanel",
    "vitest": "vitest --config ./vitest.config.ts",
    "vite": "vite",
    "migration": "ts-node ./src/scripts/migrate",
    "dev": "nodemon --exec ts-node --ignore /migrationsDiff/* src/main.ts",
    "drizzle": "nodemon --exec ts-node --ignore /migrationsDiff/* src/drizzle.ts",
    "ddev": "concurrently \"npm run tsx\" \"npm run watch\"",
    "ava": "ava src/testing/a.test.ts",
    "tap": "NODE_ENV=test tap src/schema/todos/untitledfolder/*",
    "tsx": "tsx watch src/main.ts",
    "test": "tsx ./src/testing/startTestDatabase.ts && NODE_ENV=test tsx --test src/schema/todos/lol/*",
    "testt": " NODE_ENV=test tsx --test src/testing/test.tap.ts",
    "run": "tsx ./src/testing/runTests.ts",
    "test:grep": "NODE_ENV=test mocha --config ./mocharc.json -g",
    "test:watch": "NODE_ENV=test mocha --config ./mocharc.json --watch",
    "gql-gen": "graphql-codegen --config ./src/testing/codegenConfig.ts"
  },
  "extra": {
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "db-migrate-plugin-typescript": "^2.0.0"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/sensible": "^5.5.0",
    "@fastify/session": "^10.7.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@mikro-orm/core": "^5.8.6",
    "@mikro-orm/postgresql": "^5.8.6",
    "@pothos/core": "^3.41.0",
    "@pothos/plugin-with-input": "^3.10.1",
    "@reporters/mocha": "^1.0.1",
    "@types/cls-hooked": "^4.3.3",
    "argon2": "^0.31.1",
    "body-parser": "^1.19.0",
    "case": "^1.6.3",
    "chalk": "4.1",
    "cls-hooked": "^4.2.2",
    "commander": "^11.0.0",
    "connect-pg-simple": "^9.0.0",
    "cookie-signature": "^1.1.0",
    "dataloader": "^2.0.0",
    "debounce": "^1.2.1",
    "decimal.js": "^10.4.3",
