    "express-request-id": "^3.0.0",
    "express-session": "^1.17.1",
    "fastify": "^4.26.0",
    "glob": "^10.3.10",
    "gql.tada": "^1.2.1",
    "graphql": "^16.8.1",
    "graphql-relay": "^0.10.0",
    "graphql-tag": "^2.12.6",
    "kanel": "^3.5.3",
    "kysely": "^0.26.3",
    "lodash": "^4.17.20",
    "mercurius": "^13.3.3",
